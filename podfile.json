					"version": "3.3.1"
				},
				{
					"name": "APDBidMachineAdapter",
					"version": "3.3.1.0",
					"comment": "Required"
				},
				{
					"name": "APDIABAdapter",
					"version": "3.3.1.0",
					"comment": "Required"
				}
			]
		},
		{
			"name": "appodeal_facebook_pods",
