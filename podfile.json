			"functions": [
				{
					"name": "appodeal_facebook_pods",
					"enabled": {
						"name": "constant",
						"value": false
					}
				}
			]
		},
		{
			"name": "appsflyer_pods",
			"comment": "AppsFlyer pods",
			"pods": [
				{
					"name": "AppsFlyerFramework",
					"version": "6.13.0"
				}
			]
		},
		{
			"name": "firebase_messaging_pods",
			"comment": "Firebase pods",
			"pods": [
				{
					"name": "GoogleUtilities",
					"version": "7.13.0"
				},
				{
					"name": "FirebaseMessaging",
					"version": "10.23.0"
				}
			]
		},
		{
			"name": "firebase_pods",
			"pods": [
				{
					"name": "Firebase",
					"version": "10.23.0"
				},
				{
					"name": "FirebaseCore",
					"version": "10.23.0"
				},
				{
					"name": "FirebaseCrashlytics",
					"version": "10.23.0"
				},
				{
					"name": "FirebaseAnalytics/WithoutAdIdSupport",
					"version": "10.23.0"
				}
			],
			"functions": [
				{
					"name": "firebase_messaging_pods"
				}
			],
			"script_phases": [
				{
					"comment": "Crashlytics workaround, see https://github.com/yonaskolb/XcodeGen/blob/master/Docs/FAQ.md#can-i-use-crashlytics",
					"name": "Run FirebaseCrashlytics",
					"script": "\"${PODS_ROOT}\/FirebaseCrashlytics\/run\"",
					"input_files": [
						"$(SRCROOT)\/$(BUILT_PRODUCTS_DIR)\/$(INFOPLIST_PATH)"
					]
				}
			]
		},
		{
			"name": "main_pods",
			"comment": "Main target pods",
			"pods": [
				{
					"name": "ABIcons",
					"git": "https://github.com/CleverPumpkin/abicons-ios.git",
					"tag": "0.2.1",
					"excluded_from_sdk": true
				},
				{
					"name": "SwiftLint",
					"version": "0.43.1"
				},
				{
					"name": "ESTabBarController-swift",
					"version": "2.8.0"
				},
				{
					"name": "WLSDK",
					"version": "1.4.0"
				},
				{
					"name": "WLSDK/WhiteLabelSupport",
					"version": "1.4.0"
				}
			]
		}
	],
	"abstract_targets": [
		{
			"comment": "Pods used across all modules and targets",
			"name": "General",
			"pods": [
				{
					"name": "CombineExt",
					"version": "1.8.0",
					"max_version": "< 2.0.0"
				},
				{
					"name": "R.swift",
					"version": "7.2.4",
					"max_version": "< 8.0.0"
				}
			],
			"targets": [
				{
					"name": "travel-app-notification-service-extension",
					"functions": [
						{
							"name": "firebase_messaging_pods"
						}
					]
				},
				{
					"name": "travel-app",
					"functions": [
						{
							"name": "main_pods"
						},
						{
							"name": "firebase_pods"
						},
						{
							"name": "appodeal_base_pods",
							"enabled": {
								"name": "configuration",
								"paths": [
									"advertising",
									"appodeal_api_key"
								],
								"conditions": [
									"not_nil",
									"not_empty"
								]
							}
						},
						{
							"name": "appodeal_optional_pods",
							"enabled": {
								"name": "constant",
								"value": false
							}
						},
						{
							"name": "appsflyer_pods",
							"enabled": {
								"name": "configuration",
								"paths": [
									"constants",
									"appsflyer_dev_key"
								],
								"conditions": [
									"not_nil",
									"not_empty"
								]
							}
						}
					]
				}
			]
		}
	],
	"additional_build_settings": {
		"BUILD_LIBRARY_FOR_DISTRIBUTION": "YES"
	}
}
