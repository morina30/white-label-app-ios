{
	"packages": [
		"json"
	],
	"platform": {
		"name": "ios",
		"version": "14.0"
	},
	"sources": [
		"https://github.com/appodeal/CocoaPods.git",
		"https://github.com/CocoaPods/Specs.git"
	],
	"inhibit_all_warnings": true,
	"use_frameworks": "dynamic",
	"install": {
		"method": "cocoapods",
		"options": {
			"warn_for_multiple_pod_sources": false
		}
	},
	"workspace": "travel-app.xcworkspace",
	"functions": [
